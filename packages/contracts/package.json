{
  "name": "@vaults4626/contracts",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/wagmi.ts",
  "module": "./src/wagmi.ts",
  "types": "./src/wagmi.ts",
  "files": ["dist", "src"],
  "exports": {
    ".": "./src/wagmi.ts"
  },
  "devDependencies": {
    "@tanstack/query-core": "*",
    "@wagmi/cli": "*",
    "typescript": "*"
  },
  "peerDependencies": {
    "@types/node": "*",
    "@wagmi/core": "*",
    "abitype": "*",
    "lodash": "*",
    "react": "*",
    "react-dom": "*",
    "viem": "*",
    "wagmi": "*"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "build:types": "tsc -p ./tsconfig.build.json",
    "format": "biome format . --write",
    "format-and-lint": "bun run format && bun run lint:fix:unsafe",
    "lint": "biome check .",
    "lint:all": "bun run lint:fix:unsafe && bun run lint:types",
    "lint:fix": "bun run lint --apply",
    "lint:fix:unsafe": "bun run lint --write --unsafe",
    "lint:types": ":",
    "codegen": "wagmi generate && bun run codegen:patch",
    "codegen:patch": "tsx src/codegen-patch.ts"
  }
}
